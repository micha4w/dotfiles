{
    "vim.vimrc.enable": true,
    "workbench.startupEditor": "none",
    "security.workspace.trust.enabled": false,
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.lineNumbers": "relative",
    "redhat.telemetry.enabled": false,
    "glassit.alpha": 235,
    "ansible.validation.lint.enabled": false,
    "application.shellEnvironmentResolutionTimeout": 120,
    "explorer.confirmDelete": false,
    "debug.onTaskErrors": "abort",
    "editor.minimap.size": "fill",
    "C_Cpp.formatting": "vcFormat",
    "explorer.confirmDragAndDrop": false,
    "cmake.configureOnOpen": false,
    "svelte.enable-ts-plugin": true,
    "terminal.integrated.env.linux": {},
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "editor.largeFileOptimizations": false,
    "editor.selectionClipboard": false,
    "vim.useSystemClipboard": true,
    "git.confirmSync": false,
    "console-ninja.showWhatsNew": false,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.fold"
            ],
        },
        {
            "before": [
                "<leader>",
                "F"
            ],
            "commands": [
                "editor.unfold"
            ],
        },
        {
            "before": [
                "<leader>",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ],
        },
        {
            "before": [
                "g",
                "t"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ],
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ],
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                "editor.action.addSelectionToNextFindMatch"
            ],
        },
    ],
    "editor.inlayHints.enabled": "offUnlessPressed",
    "autopep8.args": [
        "--max-line-length=200"
    ],
    "git.openRepositoryInParentFolders": "always",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "vim.handleKeys": {
        "<C-p>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false,
    },
    "C_Cpp.vcFormat.indent.lambdaBracesWhenParameter": false,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(186, 194, 222, 0.3)",
    "vim.highlightedyank.duration": 175,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "git.suggestSmartCommit": false,
    "cmake.showOptionsMovedNotification": false,
    "editor.stickyScroll.enabled": true,
    "cSpell.language": "en,de",
    "cSpell.useGitignore": false,
    "cSpell.userWords": [
        "ascii",
        "Ausschliessung",
        "clangd",
        "fileid",
        "firefox",
        "folderid",
        "Hyprland",
        "Hyprlang",
        "jsobject",
        "jsonify",
        "keybind",
        "Keybinds",
        "lastrowid",
        "linalg",
        "LONGLONG",
        "LONGPTR",
        "lvalue",
        "micha",
        "Modif",
        "MULT",
        "multipeek",
        "pango",
        "parentid",
        "PCONFIGHANDLERFUNC",
        "permissionid",
        "powerbuilder",
        "pygame",
        "socketio",
        "subfolders",
        "tempdir",
        "tempfile",
        "Timotheus",
        "tokio",
        "ungroup",
        "UNSIGNEDINTEGER",
        "UNSIGNEDLONG",
        "userid"
    ],
    "[css]": {
        "editor.defaultFormatter": "Wscats.eno"
    },
    "console-ninja.featureSet": "Community",
    "makefile.configureOnOpen": false,
    "mesonbuild.downloadLanguageServer": false,
    "update.mode": "none",
    "debug.allowBreakpointsEverywhere": true,
    "extensions.ignoreRecommendations": true,
    "mesonbuild.languageServer": null,
    "editor.fontFamily": "'DroidSansM Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "workbench.iconTheme": "catppuccin-mocha",
    "catppuccin-icons.hidesExplorerArrows": true,
    "maptz.regionfolder": {
        "[rust]": {
            "foldStart": "// #region [NAME]",
            "foldEnd": "// #endregion",
            "foldStartRegex": "^[\\s]*//[\\s]*#region[\\s]*(.*)$",
            "defaultFoldStartRegex": "^[\\s]*//[\\s]*#region[\\s]*default[\\s]*(.*)$",
            "foldEndRegex": "//[\\s]*#endregion",
        }
    },
    "window.menuBarVisibility": "toggle",
    "STM32VSCodeExtension.cubeCLT.path": "/home/micha4w/.local/share/STM32CubeCLT/",
    "STM32VSCodeExtension.projectCreator.executablePath": "/home/micha4w/.local/bin/STM32CubeMX",
    "STM32VSCodeExtension.productFinder.executablePath": "/home/micha4w/.local/bin/STMCUFinder",
    "powerbuilder-ls.trace.server": "verbose",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "terminal.integrated.defaultProfile.linux": "fish",
    "editor.minimap.enabled": false,
    "zenMode.hideLineNumbers": false,
    "lldb.showDisassembly": "never",
    "pylint.enabled": false,
    "rust-analyzer.lens.enable": false,
    "jupyter.askForKernelRestart": false,
    "nixEnvSelector.suggestion": false,
    "direnv.restart.automatic": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "rust-analyzer.check.ignore": [
        "dead_code"
    ],
    "python.analysis.typeCheckingMode": "standard",
    "files.exclude": {
        "**/.git": false
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            120
        ],
        "editor.selectionHighlight": false,
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "dart.closingLabels": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "direnv.watchForChanges": false,
    "nix.enableLanguageServer": true,
    "nix.serverSettings": {
        "nil": {
        //     "diagnostics": {
        //         "ignored": [
        //             "unused_binding",
        //             "unused_with"
        //         ],
        //     },
            "formatting": {
                "command": [
                    "nixfmt"
                ],
            },
        },
    },
    "workbench.activityBar.location": "top",
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "hjson-formatter.options": {
        "separator": false,
        "condense": 4,
        "bracesSameLine": true,
        "quotes": "strings",
        "multiline": "std",
        "space": 2
    },
    "search.useIgnoreFiles": false,
    "files.associations": {
        "Makefile": "makefile",
        "*.tpl": "mako"
    },
    "cmake.skipConfigureIfCachePresent": true,
    "veridian.serverPath": "/home/micha4w/Code/Rust/veridian/target/debug/veridian",
    "window.customTitleBarVisibility": "never",
    "window.titleBarStyle": "native",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
}